{
  "name": "whatsapp-api",
  "version": "1.0.0",
  "description": "WhatsApp API menggunakan Baileys",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:main": "nodemon --watch src src/index.js",
    "dev:broadcast": "nodemon --watch src/broadcast src/broadcast/broadcastServer.js",
    "dev:worker": "nodemon --watch src/broadcast src/broadcast/broadcastWorker.js",
    "dev:drip": "DEBUG_DRIP_WORKER=true nodemon --watch src/jobs src/jobs/dripWorker.js",
    "dev:scheduled": "nodemon --watch src/jobs src/jobs/scheduledMessageWorker.js",
    "dev:scheduled-standalone": "nodemon --watch src/jobs src/jobs/scheduledMessageWorkerStandalone.js",
    "dev:scheduled-queue": "nodemon --watch src/jobs src/jobs/scheduledMessageQueueWorker.js",
    "ensure:scheduled-queue": "./scripts/ensure_bullmq_worker.sh",
    "migrate": "node scripts/run-supabase-migration.js",
    "migrate:check": "node scripts/check-migration-status.js",
    "migrate:run": "node scripts/run-migrations-standalone.js",
    "db:setup": "node scripts/setup-database.js",
    "redis:analyze": "node find-unused-redis-keys.js",
    "redis:clean": "node clean-redis.js",
    "redis:clean:auto": "node clean-redis.js --non-interactive",
    "redis:add-ttl": "node scripts/add-ttl-to-sessions.js",
    "redis:monitor": "scripts/monitor-redis-memory.sh",
    "redis:bullmq-config": "node scripts/bullmq-config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@supabase/supabase-js": "^2.39.8",
    "@whiskeysockets/baileys": "^7.0.0-rc.6",
    "axios": "^1.6.2",
    "bull": "^4.16.5",
    "bullmq": "^5.53.3",
    "cors": "^2.8.5",
    "cron": "^4.3.1",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-prom-bundle": "^8.0.0",
    "form-data": "^4.0.0",
    "ioredis": "^5.6.1",
    "lru-cache": "^7.18.3",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "opossum": "^5.0.1",
    "pg": "^8.16.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.3",
    "redis": "^4.6.12",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "xendit-node": "^1.22.0",
    "zod": "^3.25.58"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3"
  }
}
